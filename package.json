{
  "name": "learn-nestjs",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "dotenv -e ./config/local.env nest start --watch",
    "format": "prettier --ignore-unknown --write '**/*'",
    "lint": "eslint --max-warnings 0 '**/*.{js,ts}'",
    "test": "dotenv -e ./config/local.env jest",
    "husky-install": "husky install",
    "db:migrate": "dotenv -e ./config/local.env ts-node ./scripts/migrate.ts",
    "db:gui": "dotenv -e ./config/local.env dynamodb-admin"
  },
  "dependencies": {
    "@nestjs/common": "9.0.11",
    "@nestjs/core": "9.0.11",
    "@nestjs/platform-express": "9.0.11",
    "@typedorm/common": "1.14.11",
    "@typedorm/core": "1.14.11",
    "aws-sdk": "2.1176.0",
    "dotenv": "16.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.6",
    "source-map-support": "0.5.21",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.2",
    "@nestjs/testing": "9.0.11",
    "@somq14/eslint-config": "somq14/eslint-config#v1.0.0",
    "@types/jest": "28.1.8",
    "@types/node": "17.0.31",
    "@types/supertest": "2.0.12",
    "dotenv-cli": "6.0.0",
    "dynamodb-admin": "4.4.0",
    "eslint": "8.19.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
