{
  "name": "learn-nestjs",
  "packageManager": "yarn@3.2.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "dotenv -e ./config/local.env nest start --watch",
    "format": "prettier --ignore-unknown --write '**/*'",
    "lint": "eslint --max-warnings 0 '**/*.{js,ts}'",
    "test": "dotenv -e ./config/local.env jest",
    "husky-install": "husky install",
    "db:migrate": "dotenv -e ./config/local.env ts-node ./scripts/migrate.ts",
    "db:gui": "dotenv -e ./config/local.env dynamodb-admin"
  },
  "dependencies": {
    "@nestjs/common": "8.4.6",
    "@nestjs/core": "8.4.6",
    "@nestjs/platform-express": "8.4.6",
    "@typedorm/common": "1.14.11",
    "@typedorm/core": "1.14.11",
    "aws-sdk": "2.1125.0",
    "dotenv": "16.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "source-map-support": "0.5.21",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.6",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.6",
    "@somq14/eslint-config": "somq14/eslint-config#v0.3.6",
    "@types/jest": "27.5.2",
    "@types/node": "17.0.31",
    "@types/supertest": "2.0.12",
    "dotenv-cli": "5.1.0",
    "dynamodb-admin": "4.4.0",
    "eslint": "8.15.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "12.4.2",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.3",
    "ts-jest": "27.1.5",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  }
}
